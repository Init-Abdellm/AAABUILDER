{
  "name": "aaabuilder",
  "version": "2.0.0",
  "description": "Advanced AI Agent Builder - A comprehensive framework for building AI agents with TypeScript support",
  "main": "bin/aaab.js",
  "bin": {
    "aaab": "bin/aaab.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node bin/aaab.js serve --port 5000 --watch",
    "start": "node bin/aaab.js serve --port 5000",
    "serve": "node bin/aaab.js serve --port 5000",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint lib/**/*.js",
    "lint:fix": "eslint lib/**/*.js --fix",
    "format": "prettier --write src/**/*.ts lib/**/*.js",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "doctor": "node bin/aaab.js doctor"
  },
  "keywords": [
    "ai",
    "agent",
    "workflow",
    "automation",
    "api",
    "backend",
    "typescript",
    "machine-learning",
    "llm",
    "openai",
    "anthropic",
    "gemini"
  ],
  "author": "INIT-ABDELLM",
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.1",
    "openai": "^4.28.0",
    "@google/generative-ai": "^0.1.3",
    "@anthropic-ai/sdk": "^0.18.0",
    "ollama": "^0.5.0",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "js-yaml": "^4.1.0",
    "winston": "^3.11.0",
    "typescript": "^5.3.3",
    "@pinecone-database/pinecone": "^1.1.2",
    "weaviate-client": "^3.4.0",
    "@qdrant/qdrant-js": "^1.15.1",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Init-Abdellm/AAABuilder.git"
  },
  "bugs": {
    "url": "https://github.com/Init-Abdellm/AAABuilder/issues"
  },
  "homepage": "https://github.com/Init-Abdellm/AAABuilder#readme",
  "files": [
    "bin/**/*",
    "lib/**/*",
    "templates/**/*",
    "plugins/**/*",
    "examples/**/*",
    "README.md",
    "LICENSE"
  ]
}

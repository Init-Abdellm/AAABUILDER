Windows Registry Editor Version 5.00

; Register .agent file extension with z.png icon
[HKEY_CLASSES_ROOT\.agent]
@="AAABuilder.Agent"

[HKEY_CLASSES_ROOT\AAABuilder.Agent]
@="AAABuilder Agent File"
"FriendlyTypeName"="AAABuilder Agent File"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\DefaultIcon]
@="%~dp0public\\z.png,0"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell]

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\open]
@="Open with AAABuilder"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\open\command]
@="\"%~dp0bin\\aaab.js\" \"%1\""

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\edit]
@="Edit Agent"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\edit\command]
@="notepad.exe \"%1\""

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\validate]
@="Validate Agent"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\validate\command]
@="\"%~dp0bin\\aaab.js\" validate \"%1\""

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\run]
@="Run Agent"

[HKEY_CLASSES_ROOT\AAABuilder.Agent\shell\run\command]
@="\"%~dp0bin\\aaab.js\" run \"%1\""
